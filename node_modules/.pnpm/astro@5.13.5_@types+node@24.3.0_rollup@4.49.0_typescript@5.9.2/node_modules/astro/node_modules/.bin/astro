#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ajh4ck/Documentos/web_hacking_astro/web-hacking2/node_modules/.pnpm/astro@5.13.5_@types+node@24.3.0_rollup@4.49.0_typescript@5.9.2/node_modules/astro/node_modules:/home/ajh4ck/Documentos/web_hacking_astro/web-hacking2/node_modules/.pnpm/astro@5.13.5_@types+node@24.3.0_rollup@4.49.0_typescript@5.9.2/node_modules:/home/ajh4ck/Documentos/web_hacking_astro/web-hacking2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ajh4ck/Documentos/web_hacking_astro/web-hacking2/node_modules/.pnpm/astro@5.13.5_@types+node@24.3.0_rollup@4.49.0_typescript@5.9.2/node_modules/astro/node_modules:/home/ajh4ck/Documentos/web_hacking_astro/web-hacking2/node_modules/.pnpm/astro@5.13.5_@types+node@24.3.0_rollup@4.49.0_typescript@5.9.2/node_modules:/home/ajh4ck/Documentos/web_hacking_astro/web-hacking2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
